{
  "name": "@vtaits/react-router-dom-v5-fake-browser",
  "version": "1.0.0",
  "description": "Embeddable fake browser for react applications based on react-router-dom version 5",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "keywords": [
    "fake",
    "browser",
    "embeddable",
    "react-router"
  ],
  "repository": "git@github.com:vtaits/react-fake-browser.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest run --coverage.enabled --coverage.all --coverage.include=src --coverage.exclude=src/__stories__",
    "test:unit:watch": "vitest",
    "test": "yarn lint && yarn test:ts && yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-router-dom": "^5.0.0",
    "styled-components": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.4",
    "@types/react": "^19.1.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^19.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "react": "^19.1.0",
    "react-router-dom": "^5.3.4",
    "react-test-renderer": "^19.1.0",
    "styled-components": "^6.1.19",
    "tsup": "^8.5.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@vtaits/react-fake-browser-ui": "^1.0.0"
  }
}
