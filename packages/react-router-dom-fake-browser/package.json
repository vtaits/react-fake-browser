{
  "name": "@vtaits/react-router-dom-fake-browser",
  "version": "1.0.0",
  "description": "Embeddable fake browser for react applications based on react-router-dom",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typings": "dist/index.d.ts",
  "keywords": [
    "fake",
    "browser",
    "embeddable",
    "react-router"
  ],
  "repository": "git@github.com:vtaits/react-fake-browser.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.47",
    "@types/react-test-renderer": "^18.0.7",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "react-test-renderer": "^18.2.0",
    "styled-components": "^6.1.7",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1"
  },
  "dependencies": {
    "@vtaits/react-fake-browser-ui": "^1.0.0"
  }
}
